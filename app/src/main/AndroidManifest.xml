<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iwatch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Activities.PersonDetails"></activity>
        <activity android:name=".Activities.SeasonDetails">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SerieDetails" />
        <activity android:name=".Activities.MovieDetails" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.SignUp" />
        <activity android:name=".Activities.Home" />
        <activity
            android:name=".Activities.EditProfile"
            android:parentActivityName=".Activities.Home" />
        <activity android:name=".Activities.ConfirmRegistration" />
    </application>

</manifest>